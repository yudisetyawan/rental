<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aplikasi.mobil">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="RENTAL MOBIL"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.aplikasi.mobil.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.aplikasi.mobil.BuatKontak"
            android:label="Daftar Rental Mobil">
        </activity>
        <activity android:name="com.aplikasi.mobil.LihatKontak"
            android:label="Lihat Mobil">
        </activity>
        <activity android:name="com.aplikasi.mobil.UpdateKontak"
            android:label="Edit Mobil">
        </activity>
    </application>

</manifest>